<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mrip.funmindgame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mrip.funmindgame.Congratulations"></activity>

        <activity android:name="com.mrip.funmindgame.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mrip.funmindgame.LuckHard"
            android:label="Luck Game Hard"
            android:theme="@style/AppTheme"/>
        <activity android:name="com.mrip.funmindgame.LuckMedium"
            android:label="Luck Game Medium"
            android:theme="@style/AppTheme"/>
        <activity android:name="com.mrip.funmindgame.LuckEasy"
            android:label="Luck Game Easy"
            android:theme="@style/AppTheme"/>
        <activity android:name="com.mrip.funmindgame.MindGameFinal"
            android:label="Mind Game"
            android:theme="@style/AppTheme"/>
        <activity android:name="com.mrip.funmindgame.MindGameThird"
            android:label="Mind Game"
            android:theme="@style/AppTheme"/>
        <activity android:name="com.mrip.funmindgame.LuckGame"
            android:label="Luck Game"
            android:theme="@style/AppTheme"/>
        <activity android:name="com.mrip.funmindgame.MindGame"
            android:label="Mind Game"
            android:theme="@style/AppTheme"/>
    </application>

</manifest>